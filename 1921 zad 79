#ege19-21  zad 79
def F(x, p, win):
    if 20 <= x <= 30: return p%2 == win%2
    if x > 30: return p%2 != win%2
    if p >= win: return False
    lst = [F(x+1, p+1, win), F(x*2, p+1, win)]
    if (p+1)%2 == win%2:
        return any(lst)
    else:
        return all(lst)

for s in range(1, 20):
    if F(s, 0, 4):
        print(s)
    
